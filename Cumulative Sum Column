LIQUIDADO TOTAL = 
    SUMX(
        FILTER(
            '_BD_CONTAS_A_RECEBER',
            EARLIER('_BD_CONTAS_A_RECEBER'[CONTRATO])='_BD_CONTAS_A_RECEBER'[CONTRATO]
            &&EARLIER('_BD_CONTAS_A_RECEBER'[DATA BASE]) >= '_BD_CONTAS_A_RECEBER'[DATA BASE]
        ),
        '_BD_CONTAS_A_RECEBER'[BOLETIM]
    )
